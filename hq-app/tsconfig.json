{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
      // "es2020"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@/pages/*": ["pages/*"],
      "@/containers/*": ["containers/*"],
      "@/lib/*": ["lib/*"],
      "@/utils/*": ["utils/*"],
      "@images/*": ["images/*"]
    }
    // "noUnusedLocals": true,
    // "noUnusedParameters": true,
    // "preserveConstEnums": true,
    // "removeComments": false,
    // "sourceMap": true,
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "declaration.d.ts",
    "**/images/",
    "containers/Finishedproduct/Add/Steps/generalcopy.js",
    "containers/Finishedproduct/Add/Steps/generalcopy.js"
  ],
  "exclude": [
    "node_modules"
    // ".next"
  ]
}
